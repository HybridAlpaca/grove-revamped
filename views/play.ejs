<!doctype html>
<html>

<head>
    <title>The Grove</title>
    <link rel='icon' href='/assets/icons/tree.png'>
    <link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/css/materialize.min.css">
    <link rel='stylesheet' href='/css/play.css'>
</head>

<body>
    <div id="blocker" class='pntrlk'>

        <div id="instructions" class='pntrlk'>
            <img src='/assets/img/loader.gif'>
            <br>
            <p id='loaded'>[ loading ]</p>
        </div>

    </div>

    <div id='overlay' style='display:none;'></div>
    <div id='vignette' style='display:none;'></div>

    <img id='crosshair' src='/assets/img/crosshair/crosshair.png' width=30>
    <div id='label'>[ loading ]</div>

    <div id='gui'>
        <h3 id='gui-title'>[ loading ]</h3>
        <hr id='gui-hr'>
        <div id='gui-content'>[ loading ]</div>
        <div id='gui-buttons'>
            <button class='btn pntrlk' onclick='$("#gui").fadeOut(400)'>Sounds Good</button>
            <button class='btn pntrlk' onclick='$("#gui").fadeOut(400)'>Close</button>
        </div>
    </div>

    <img id='quests' src='/assets/icons/stabbed-note.png' width=50>

    <div id='hud'>
        <p id='hp'>[ loading ]</p>
    </div>

    <script src="//code.jquery.com/jquery-3.2.1.min.js" integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4=" crossorigin="anonymous"></script>
    <script src="//cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/js/materialize.min.js"></script>

    <script>
        window.console.__error = window.console.error;
        window.console.__warn = window.console.warn;
        window.console.error = function(out) {
            if (!/looks like it points into the shape/.test(out))
                console.__error(out);
        };
        window.console.warn = function(out) {
            if (!/^\.vertices\[/.test(out))
                console.__warn(out);
        };
    </script>

    <script src='/js/vendor.min.js'></script>
    <script src='/js/main.min.js'></script>
</body>

</html>
